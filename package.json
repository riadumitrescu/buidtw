    "autoprefixer": "^10.4.21",
    "cssnano": "^7.0.6",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "gh-pages": "^6.3.0",
    "postcss": "^8.5.3",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "sass": "^1.86.1",
    "terser": "^5.27.2",
    "vite": "^5.1.3"
  }
